int inputPin = 2;
bool state = false;

void setup()
{
  Serial.begin(9600);
  pinMode(LED_BUILTIN, OUTPUT);
  pinMode(inputPin, INPUT);
  
  attachInterrupt(digitalPinToInterrupt(inputPin), swap, CHANGE);
}

void loop()
{
  delay(500);
}

void swap()
{
  state = !state;
  digitalWrite(LED_BUILTIN, state);
  Serial.println("Interrupted");
}
